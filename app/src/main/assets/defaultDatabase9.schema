
CREATE TABLE IF NOT EXISTS `fylker` (
    `fylkeId` INTEGER NOT NULL,
    `fylkeName` TEXT NOT NULL,
    `color` INTEGER NOT NULL,
    PRIMARY KEY(`fylkeId`)
);

CREATE TABLE IF NOT EXISTS `points` (
    `x` INTEGER NOT NULL,
    `y` INTEGER NOT NULL,
    `fylkeId` INTEGER NOT NULL,
    PRIMARY KEY(`x`, `y`),
    FOREIGN KEY(`fylkeId`) REFERENCES `fylker`(`fylkeId`) ON UPDATE NO ACTION ON DELETE NO ACTION 
);

CREATE TABLE IF NOT EXISTS `sources` (
    `sourceId` TEXT NOT NULL,
    `lat` REAL NOT NULL,
    `lon` REAL NOT NULL,
    `fylkeId` INTEGER NOT NULL,
    PRIMARY KEY(`sourceId`),
    FOREIGN KEY(`fylkeId`) REFERENCES `fylker`(`fylkeId`) ON UPDATE NO ACTION ON DELETE NO ACTION
);

CREATE TABLE IF NOT EXISTS `sourceForecast` (
    `sourceId` TEXT NOT NULL,
    `timeUpdated` INTEGER NOT NULL,
    `timestamp` INTEGER NOT NULL,
    `windSpeed` REAL NOT NULL,
    `windDirection` REAL NOT NULL,
    PRIMARY KEY(`sourceId`, `timeUpdated`, `timestamp`),
    FOREIGN KEY(`sourceId`) REFERENCES `sources`(`sourceId`) ON UPDATE NO ACTION ON DELETE NO ACTION
);

CREATE TABLE IF NOT EXISTS `sourceHistory` (
    `sourceId` TEXT NOT NULL,
    `timestamp` INTEGER NOT NULL,
    `windSpeed` REAL NOT NULL,
    `windDirection` REAL NOT NULL,
    `windTurbineCount` INTEGER NOT NULL,
    PRIMARY KEY(`sourceId`, `timestamp`),
    FOREIGN KEY(`sourceId`) REFERENCES `sources`(`sourceId`) ON UPDATE NO ACTION ON DELETE NO ACTION
);

CREATE TABLE IF NOT EXISTS `windturbines` (
    `id` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    `sourceId` TEXT NOT NULL,
    `timestamp` INTEGER NOT NULL,
    FOREIGN KEY(`sourceId`) REFERENCES `sources`(`sourceId`) ON UPDATE NO ACTION ON DELETE NO ACTION
);

CREATE TABLE IF NOT EXISTS `settings` (
    `dataKey` TEXT NOT NULL,
    `dataValue` INTEGER NOT NULL,
     PRIMARY KEY(`dataKey`)
);

